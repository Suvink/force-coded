{
    "name": "force-coded",
    "version": "0.0.1",
    "description": "A Node.js SDK for CodeForces and Sample Apps",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "lerna run build --stream",
        "build:dev": "lerna run build:dev --stream",
        "prebuild": "yarn run bootstrap",
        "bootstrap": "yarn install && lerna bootstrap",
        "remove-node-modules": "npx lerna exec -- rimraf -rf node_modules && rimraf -rf node_modules",
        "remove-package-lock": "npx lerna exec -- rimraf -rf yarn.lock && rimraf -rf yarn.lock",
        "lint": "lerna run lint",
        "fix-lint": "lerna run fix-lint",
        "clean": "lerna run clean --stream",
        "clean-all": "yarn run remove-package-lock && yarn run remove-node-modules",
        "bump-patch-version": "lerna version patch --yes",
        "bump-minor-version": "lerna version minor --yes",
        "bump-major-version": "lerna version major --yes"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nandulaperera/force-coded.git"
    },
    "keywords": [
        "CP",
        "Codeforces",
        "Competitive Programing",
        "Node.js"
    ],
    "author": "Nandula Perera",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/nandulaperera/force-coded/issues"
    },
    "homepage": "https://github.com/nandulaperera/force-coded#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.6.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.25.4",
        "lerna": "^4.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.2"
    }
}
